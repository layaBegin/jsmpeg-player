{
  "name": "vue-jsmpeg-player",
  "version": "1.3.0",
  "description": "基于jsmpeg.js二次开发的流媒体播放器",
  "private": false,
  "author": "云帆 <cloudsail.studio@outlook.com>",
  "license": "MIT",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vCloudSail/jsmpeg-player"
  },
  "bugs": {
    "url": "https://github.com/vCloudSail/jsmpeg-player/issues"
  },
  "scripts": {
    "dev": "vite --config vite.demo.config.js",
    "build": "vite build",
    "server": "node ./server/index.js",
    "lint": "vue-cli-service lint",
    "clear": "rimraf .eslintcache && rimraf package-lock.json && rimraf yarn.lock && rimraf pnpm-lock.yaml && rimraf node_modules",
    "reinstall": "npm run clear && yarn",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "core-js": "^3.24.1",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vitejs/plugin-vue2-jsx": "^1.1.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^1.3.4",
    "autoprefixer": "^10.4.14",
    "element-ui": "^2.15.0",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "~1.32.13",
    "terser": "^5.19.2",
    "unplugin-vue-components": "^0.25.0",
    "vconsole": "^3.15.0",
    "vite": "^4.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-libcss": "^1.1.1",
    "vite-plugin-require-transform": "^1.0.9",
    "vitest": "^0.29.1",
    "vue": "^2.7.0"
  },
  "peerDependencies": {
    "core-js": "^3.0.0",
    "vue": "^2.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "keywords": [
    "rtsp",
    "rtmp",
    "m3u8",
    "jsmpeg",
    "ffmpeg",
    "html5",
    "player",
    "video",
    "websocket",
    "mpeg1",
    "mp2",
    "vue"
  ],
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,vue,css}": [
      "prettier --config .prettierrc.js --write",
      "git add"
    ]
  }
}
